[flake8]
# W191: tabs instead of spaces
# E302: expected 2 blank lines, found 0
# E265: block comment should start with '# '
# E241: multiple spaces after ':'
# F403: from . import *
# E401: multiple imports on one line
# E502: ???
max-line-length = 160
ignore = F403,E401,E501
exclude = .git,__pycache__,*log*,.pyvenv
max-complexity = 12